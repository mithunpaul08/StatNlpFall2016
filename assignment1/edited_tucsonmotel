            jQuery(document).ready(function($){
                var gallery_ad_f3305b92_238f_5a63_ad01_c474ccaddb56_photo_modal = false;
                var gallery_changed_f3305b92_238f_5a63_ad01_c474ccaddb56_photo_modal = false;
                $('#gallery-items-f3305b92-238f-5a63-ad01-c474ccaddb56-photo-modal').owlCarousel({
                    items: 1,
                    autoHeight: true,
                    lazyLoad: true,
                    loop: false,
                    navRewind: true,
                    responsiveRefreshRate: 500,
                    
                    
                    dots : false,
                    onChanged: function(){
                        if( gallery_ad_f3305b92_238f_5a63_ad01_c474ccaddb56_photo_modal ){
                                $('#tnt-gallery-ad').hide();
                                try {
            
            var bOnlyDFP = false, bIsOAS = false;
            if( typeof __tnt.ads.dfp != 'undefined' ){
                    bOnlyDFP = true;
                googletag.cmd.push(function(){
                        googletag.pubads().clear([__tnt.ads.dfp.slots['fixed-big-ad-gallery'].dfp_slot]);
                    });}} catch(e) {
            __tnt.log(e);
        }
                                gallery_ad_f3305b92_238f_5a63_ad01_c474ccaddb56_photo_modal = false;
                                $sGalleryAdInView = false;
                            }
                            
                            if( $('#gallery-items-f3305b92-238f-5a63-ad01-c474ccaddb56-photo-modal .photo-container').eq(this._current).hasClass('photo-ad') ){
                                /** if any gallery in page is on ad slide and another gallery serves ad, advance last instance */
                                if( typeof $sGalleryAdInView !== 'undefined' && $sGalleryAdInView && $sGalleryAdInView !== '#gallery-items-f3305b92-238f-5a63-ad01-c474ccaddb56-photo-modal'){
                                    $($sGalleryAdInView).data('owlCarousel').next();
                                }
                                var adHTML = $('#tnt-gallery-ad').detach().show();
                                $('#gallery-items-f3305b92-238f-5a63-ad01-c474ccaddb56-photo-modal .photo-container').eq(this._current).find('.gallery-ad').append(adHTML);

                                try {
            
            var bOnlyDFP = false, bIsOAS = false;
            if( typeof __tnt.ads.dfp != 'undefined' ){
                    bOnlyDFP = true;
                
                   amznads.getAdsCallback('3266', function (){
                       amznads.setTargetingForGPTAsync('amznslots');
                        googletag.cmd.push(function(){
                            googletag.pubads().clear([__tnt.ads.dfp.slots['fixed-big-ad-gallery'].dfp_slot]);
                            googletag.pubads().refresh([__tnt.ads.dfp.slots['fixed-big-ad-gallery'].dfp_slot]);
                        });
                       googletag.pubads().clearTargeting('amznslots');
                   });}
                /* reloadByIds will return false/0 if the ad slot called for hasn't actually been loaded to the page. */
                if( typeof __tnt.ads.oas != 'undefined' ) bIsOAS = !!__tnt.ads.oas.reloadByIds(['fixed-big-ad-gallery']);
            
                if( !bOnlyDFP && !bIsOAS && typeof TNCMS.AdManager != 'undefined' ){
                        TNCMS.AdManager.refresh({region: 'fixed-big-ad-gallery'});}
            } catch(e) {
            __tnt.log(e);
        }

                                gallery_ad_f3305b92_238f_5a63_ad01_c474ccaddb56_photo_modal = true;
                                /** set instance of carousel containing active ad slide. */
                                $sGalleryAdInView = '#gallery-items-f3305b92-238f-5a63-ad01-c474ccaddb56-photo-modal';
                            }
                            if( gallery_changed_f3305b92_238f_5a63_ad01_c474ccaddb56_photo_modal ){
                                
                            }
                            gallery_changed_f3305b92_238f_5a63_ad01_c474ccaddb56_photo_modal = true;
                        

                        
                            var currItem = $('#gallery-items-f3305b92-238f-5a63-ad01-c474ccaddb56-photo-modal .photo-container').eq(this._current).data('count');
                            if(currItem){
                                $('#carousel-f3305b92-238f-5a63-ad01-c474ccaddb56-photo-modal .current-count').html(currItem);
                            }
                            $('.modal.in, #carousel-f3305b92-238f-5a63-ad01-c474ccaddb56-photo-modal .item').removeClass('hover');
                        

                        

                    },
                    onTranslated:function(){
                        
                        
                        
                            __tnt.trackEvent($('#gallery-items-f3305b92-238f-5a63-ad01-c474ccaddb56-photo-modal .photo-container').eq(this._current).data('track-object'));
                        

                        
                        

                        

                    }
                });

                
                var photo_carousel_f3305b92_238f_5a63_ad01_c474ccaddb56_photo_modal = $('#gallery-items-f3305b92-238f-5a63-ad01-c474ccaddb56-photo-modal').data('owlCarousel');

                $('#carousel-f3305b92-238f-5a63-ad01-c474ccaddb56-photo-modal .photo-carousel-control.left').on(__tnt.client.clickEvent, function(e){
                    photo_carousel_f3305b92_238f_5a63_ad01_c474ccaddb56_photo_modal.prev();
                });

                $('#carousel-f3305b92-238f-5a63-ad01-c474ccaddb56-photo-modal .photo-carousel-control.right').on(__tnt.client.clickEvent, function(e){
                    photo_carousel_f3305b92_238f_5a63_ad01_c474ccaddb56_photo_modal.next();
                });
                    
                    });
        </script>

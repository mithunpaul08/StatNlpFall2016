                    },
                    onTranslated:function(){
                        
                        
                        
                            __tnt.trackEvent($('#gallery-items-f3305b92-238f-5a63-ad01-c474ccaddb56 .photo-container').eq(this._current).data('track-object'));
                        

                        
                        

                        

                    }
                });

                
                var photo_carousel_f3305b92_238f_5a63_ad01_c474ccaddb56 = $('#gallery-items-f3305b92-238f-5a63-ad01-c474ccaddb56').data('owlCarousel');

                $('#carousel-f3305b92-238f-5a63-ad01-c474ccaddb56 .photo-carousel-control.left').on(__tnt.client.clickEvent, function(e){
                    photo_carousel_f3305b92_238f_5a63_ad01_c474ccaddb56.prev();
                });

                $('#carousel-f3305b92-238f-5a63-ad01-c474ccaddb56 .photo-carousel-control.right').on(__tnt.client.clickEvent, function(e){
                    photo_carousel_f3305b92_238f_5a63_ad01_c474ccaddb56.next();
                });});
        </script>
                </div>
            

            

            


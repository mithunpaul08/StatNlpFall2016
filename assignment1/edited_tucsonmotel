                    });}} catch(e) {
            __tnt.log(e);
        }
                                gallery_ad_f3305b92_238f_5a63_ad01_c474ccaddb56 = false;
                                $sGalleryAdInView = false;
                            }
                            
                            if( $('#gallery-items-f3305b92-238f-5a63-ad01-c474ccaddb56 .photo-container').eq(this._current).hasClass('photo-ad') ){
                                /** if any gallery in page is on ad slide and another gallery serves ad, advance last instance */
                                if( typeof $sGalleryAdInView !== 'undefined' && $sGalleryAdInView && $sGalleryAdInView !== '#gallery-items-f3305b92-238f-5a63-ad01-c474ccaddb56'){
                                    $($sGalleryAdInView).data('owlCarousel').next();
